{
    // Environments
    "node" : true,
    "es5" : true,
    "browser" : true,
    "devel": true,
    "predef": [
        "Popcorn",
        "requirejs",
        "require",
        "define",
        "Range",
        "DOMException"
    ],

    // Enforcing
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "latedef": true,
    "noempty": true,
    "nonew": true,
    "undef": true,
    "trailing": true,

    // Relaxing
    "evil": true,
    "sub": true,
    "strict": false,
    "nonstandard": true
}
