    "../external/popcorn-js/players/soundcloud/popcorn.soundcloud.js",
    "../external/popcorn-js/plugins/wikipedia/popcorn.wikipedia.js",
    "../external/popcorn-js/plugins/twitter/popcorn.twitter.js",
    "../external/popcorn-js/plugins/text/popcorn.text.js",
    "../external/popcorn-js/plugins/googlemap/popcorn.googlemap.js",
