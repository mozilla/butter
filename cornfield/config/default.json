{
  "server" : {
    "bindIP" : "localhost",
    "bindPort" : "8888"
  },
  "logger" : {
    "format" : "dev"
  },
  "session" : {
    "secret": "thisisareallyreallylongsecrettoencryptcookies",
    "maxAge": "3600000"
  },
  "staticMiddleware": {
    "maxAge": "0"
  },
  "dirs": {
    "wwwRoot": "../",
