doctype 5
html
  head
    title Dashboard
    style
      th {
        text-align: left
      }
  body
    h1 My Dashboard -
      = user.email
    table
      tr
        th Name
        th Template
      each project in projects
        tr
          td
            a(href= project.href)
              = project.name
          td
            = project.template
