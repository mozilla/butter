{
  "name": "basic",
  "template": "index.html",
  "baseDir": "../../",
  "savedDataUrl": "saved-data.json",
  "plugin": {
    "plugins": [
      {
        "type": "titles",
        "path": "{{baseDir}}templates/assets/plugins/titles/popcorn.titles.js",
        "icon": "{{baseDir}}templates/assets/plugins/titles/titles-icon.png"
      },
      {
        "type": "photo",
        "path": "{{baseDir}}templates/assets/plugins/photo/popcorn.photo.js",
        "icon": "{{baseDir}}templates/assets/plugins/image/image-icon.png"
      },
      {
        "type": "popup",
        "path": "{{baseDir}}templates/assets/plugins/popup/popcorn.popup.js",
        "icon": "{{baseDir}}templates/assets/plugins/popup/popup-icon.png"
      },
      {
        "type": "googlemap",
        "path": "{{baseDir}}templates/assets/plugins/googlemap/popcorn.googlemap.js",
        "icon": "{{baseDir}}resources/icons/map.png"
      },
      {
        "type": "twitter",
        "path": "{{baseDir}}templates/assets/plugins/twitter/popcorn.twitter.js",
        "icon": "{{baseDir}}templates/assets/plugins/twitter/twitter-icon.png"
      },
      {
        "type": "image",
        "path": "{{baseDir}}templates/assets/plugins/image/popcorn.image.js",
        "icon": "{{baseDir}}templates/assets/plugins/image/image-icon.png"
      },
      {
        "type": "repeat",
        "path": "{{baseDir}}templates/assets/plugins/repeat/popcorn.repeat.js",
        "icon": "{{baseDir}}resources/icons/link.png"
      },
      {
        "type": "stop",
        "path": "{{baseDir}}templates/assets/plugins/stop/popcorn.stop.js"
      },
      {
        "type": "jump",
        "path": "{{baseDir}}templates/assets/plugins/jump/popcorn.jump.js"
      }
    ],
    "defaults": [
      "titles",
      "photo",
      "popup",
      "googlemap",
      "twitter",
      "image",
      "repeat",
      "stop",
      "jump"
    ]
  },
  "editor": {
    "googlemap": "{{baseDir}}editors/googlemap-editor.js",
    "popup": "{{baseDir}}templates/assets/editors/popup/popup-editor.js",
    "image": "{{baseDir}}templates/assets/editors/image/popcorn.image.editor.js",
    "titles": "{{baseDir}}templates/assets/editors/titles/titles-editor.js"
  },
  "maxPluginZIndex": 1000
}
