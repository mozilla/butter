{
  "name": "newscaster",
  "title": "Newscaster",
  "template": "../index.html",
  "baseDir": "../../../",
  "savedDataUrl": "butter/saved-data.json",
  "editor": {
    "default": "{{baseDir}}editors/default-editor.html",
    "photo": "{{baseDir}}templates/supported/shared/editors/photo-editor.html",
    "googlemap": "{{baseDir}}editors/googlemap-editor.html"
  },
  "plugin": {
    "plugins": [
      {
        "type": "text",
        "path": "{{baseDir}}external/popcorn-js/plugins/text/popcorn.text.js"
      },
      {
        "type": "titlecard",
        "path": "{{baseDir}}templates/supported/shared/plugins/titlecard/popcorn.titlecard.js"
      },
      {
        "type": "photo",
        "path": "{{baseDir}}templates/supported/shared/plugins/photo/popcorn.photo.js"
      },
      {
        "type": "zoink",
        "path": "{{baseDir}}templates/supported/shared/plugins/zoink/popcorn.zoink.js"
      },
      {
        "type": "googlemap",
        "path": "{{baseDir}}external/popcorn-js/plugins/googlemap/popcorn.googlemap.js"
      },
      {
        "type": "flickr",
        "path": "{{baseDir}}templates/supported/shared/plugins/flickr/popcorn.flickr.js"
      }
    ],
    "defaults": [
      "text",
      "titlecard",
      "photo",
      "zoink",
      "shrink",
      "flickr",
      "googlemap"
    ]
  }
}
