{
  "name": "newscaster",
  "baseDir": "../../../",
  "editor": {
    "default": "{{baseDir}}editors/default-editor.html",
    "image2": "{{baseDir}}templates/supported/shared/editors/image-editor.html"
  },
  "plugin": {
    "plugins": [
      {
        "type": "text",
        "path": "{{baseDir}}external/popcorn-js/plugins/text/popcorn.text.js"
      },
      {
        "type": "context-switcher",
        "path": "{{baseDir}}templates/supported/shared/plugins/context-switcher/popcorn.context-switcher.js"
      },
      {
        "type": "titlecard",
        "path": "{{baseDir}}templates/supported/shared/plugins/titlecard/popcorn.titlecard.js"
      },
      {
        "type": "image2",
        "path": "{{baseDir}}templates/supported/shared/plugins/image2/popcorn.image2.js"
      }
    ],
    "defaults": [
      "text",
      "titlecard",
      "image2",
      "context-switcher"
    ]
  }
}
